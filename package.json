{
  "name": "react-boilerplate",
  "version": "3.2.0",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-prod-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:gh-pages": "cross-env NODE_ENV=gh-pages webpack --progress --colors",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "fix": "eslint --fix src/**/*.js src/**/*.jsx"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bulma": "^0.7.4",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.0.0",
    "react-tabify": "0.1.24",
    "redux": "^4.0.1",
    "redux-entity": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "del": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^6.1.4",
    "node-sass": "^7.0.0",
    "nyc": "^14.0.0",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.8.4",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
